class Solution{
    public int SubtractProductAddSum(int n){
    int sum=0;
        int product=1;
        while(n>0){
            int digit=n%10;
            sum+ =digit;
            product*=digit;
            n/=10;
        }
      return product=sum;  
    }
}
---------------------------------------------------------------------------------------
OUTPUT:
n=234
15
n=4421
21
